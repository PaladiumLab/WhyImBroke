{
    "name": "whyimbroke",
    "private": true,
    "version": "0.0.0",
    "type": "commonjs",
    "scripts": {
        "start": "node dist/api/server.js",
        "build": "npm run build:server && npm run build:client",
        "build:server": "tsc -p tsconfig.server.json",
        "build:client": "tsc -p tsconfig.client.json && vite build --outDir dist/client",
        "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
        "dev:server": "ts-node-dev --respawn --transpile-only --project tsconfig.server.json src/api/server.ts",
        "dev:client": "vite",
        "lint": "eslint .",
        "preview": "vite preview --outDir dist/client"
    },
